<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!-- Sequence for Service Order. -->
    <record id="sequence_service_order" model="ir.sequence">
        <field name="name">Service Order Sequence</field>
        <field name="code">service.order.sequence</field>
        <field name="prefix">SER/%(year)s/</field>
        <field name="padding">5</field>
        <field name="implementation">no_gap</field>
    </record>

    <record id="sequence_fleet_breakdown" model="ir.sequence">
        <field name="name">Fleet Breakdown Sequence</field>
        <field name="code">fleet.breakdown.sequence</field>
        <field name="prefix">BRK</field>
        <field name="padding">5</field>
    </record>
</odoo>